import java.util.ArrayList;

public class Main {
    public static void main(String[] args){
        //Create an array list to share names
        ArrayList<String> names = new ArrayList<>();

        names.add("Lux");
        names.add("Anex");
        names.add("Sma");
        names.add("Nanka");
        names.add("Anitha");
        names.add("Ruth");
        names.add("Matt");

        int indexSma = names.indexOf("Sma");
        if (indexSma != -1){
            names.set(indexSma, "Mirati");
        }
        int indexAnitha = names.indexOf("Anitha");
        if (indexAnitha != -1) {
            names.set(indexAnitha, "Hope");
        }

        names.remove("Lux");
        names.remove("Matt");

        //Print them vertically
        for (int i=0; i< names.size(); i++){
            System.out.println(names.get(i));
        }
    }
}
